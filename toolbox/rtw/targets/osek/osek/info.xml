<list>

<listitem>
<label>Help</label>
<callback>doc osek/</callback>
<icon>$toolbox/matlab/icons/book_sim.gif</icon>
</listitem>

<listitem>
<label>Demos</label>
<callback>demo simulink 'Embedded Target for OSEK/VDX&#174;'</callback>
<icon>$toolbox/matlab/icons/demoicon.gif</icon>
</listitem>

<listitem>
<label>Environment Setup</label>
<callback>helpview([docroot '/toolbox/osek/osek.map'],'osek_setup_verify')</callback>
<icon>$toolbox/matlab/icons/book_sim.gif</icon>
</listitem>

<listitem>
<label>OSEK Library</label>
<callback>oseklib</callback>
<icon>$toolbox/simulink/simulink/simulinkicon.gif</icon>
</listitem>

<listitem>
<label>Example Driver</label>
<callback>osekdrivers</callback>
<icon>$toolbox/simulink/simulink/simulinkicon.gif</icon>
</listitem>

<listitem>
<label>OSEK Target Preferences</label>
<callback>osekeditprefs</callback>
