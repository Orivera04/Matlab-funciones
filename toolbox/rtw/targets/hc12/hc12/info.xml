<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>14</matlabrelease>
<name>Embedded Target for Motorola&#174; HC12</name>
<type>simulink</type>
<icon>$toolbox/matlab/icons/boardicon.gif</icon>
<help_location>$docroot/toolbox/hc12</help_location>

<list>

<listitem>
<label>Help</label>
<callback>doc hc12/</callback>
<icon>$toolbox/matlab/icons/book_sim.gif</icon>
</listitem>

<listitem>
<label>Demos</label>
<callback>load_system('hc12drivers');open_system('hc12drivers/HC12 Demos');</callback>
<icon>$toolbox/matlab/icons/demoicon.gif</icon>
</listitem>

<listitem>
<label>Environment Setup</label>
<callback>helpview([docroot '/toolbox/hc12/hc12.map'],'hc12_setup_verify')</callback>
<icon>$toolbox/matlab/icons/book_sim.gif</icon>
</listitem>

<listitem>
<label>HC12 Driver Library</label>
<callback>hc12drivers</callback>
<icon>$toolbox/simulink/simulink/simulinkicon.gif</icon>
</listitem>

<listitem>
<label>HC12 Target Preferences</label>
<callback>hc12editprefs</callback>
<icon>$toolbox/rtw/targets/common/configuration/resource/target_launchpad_icon.gif</icon>
</listitem>

<listitem>
<label>Product Page (Web)</label>
<callback>web('http://www.mathworks.com/products/target_hc12/', '-browser');</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

</list>

</productinfo>
