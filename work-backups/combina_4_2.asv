function b=combina_4(r)
%Combinaciones ternarias de n elementos
n=numel(r);
a=combina3(r);
m=1;
for i=1:n-3
    for j=i+1:n-2
        for k=j+1:n-1
            for l=k+1:n
            b(m,l)=a(i,j)*r(l);
            m=m+1;
        end
        %m=m+1;
    end
    %m=m+1;
    end
end
b

