%Matriz de enlaces o lados
N=input('No de vértices del grafo:');
MA=zeros(N); %Matriz de adyacencia.
             %ME: Matriz de enlaces.
for i=1:N-1
    ind=input(['vértices conectados con v#',num2str(i),' ']);
    MA(i,ind)=1;
end
k=1; e1=0;e2=0;
for i=1:N
for j=1:N  
    if MA(i,j)==1
       e1(k)=i; e2(k)=j; k=k+1;
    end                         
end
end
E=[e1',e2'];
e=frec(e1);
e=[e,0];
